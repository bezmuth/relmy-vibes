{
    "id" : "uk.bezmuth.RelmyVibes",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "48",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "relmyvibes",
    "finish-args" : [
        "--share=ipc",
        "--share=network",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--socket=session-bus",
        "--socket=pulseaudio",
        "--device=dri",
        "--env=RUST_LOG=gtk_rust_template=debug",
        "--env=G_MESSAGES_DEBUG=none",
        "--env=RUST_BACKTRACE=1"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "build-args" : [
            "--share=network",
            "--socket=fallback-x11",
            "--socket=wayland",
            "--socket=session-bus",
            "--socket=pulseaudio",
            "--device=dri",
            "--talk-name=uk.bezmuth.RelmyVibes"
        ],
        "env" : {
            "RUST_BACKTRACE" : "1",
            "RUST_LOG" : "relmyvibes=debug"
        }
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "relmyvibes",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "file:///home/bezmuth/Projects"
                }
            ],
            "config-opts" : [
                "--libdir=lib"
            ]
        }
    ]
}
